        "required": true
      },
      "devices": {
        "type": "array",
        "title": "Devices",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string",
              "title": "Device Name",
              "required": true
            },
            "mac": {
              "type": "string",
              "title": "Device MAC Address",
              "required": false
            }
          }
        }
      }
    }
  }
}
